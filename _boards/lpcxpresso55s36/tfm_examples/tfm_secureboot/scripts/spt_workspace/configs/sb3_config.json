{
    "family": "lpc55s3x",
    "containerKeyBlobEncryptionKey": "cust_mk_sk.txt",
    "description": "LPC55S36 SB3",
    "firmwareVersion": "0x1",
    "rootCertificate0File": "../keys/ROT1_p384.pem",
    "rootCertificate1File": "../keys/ROT2_p384.pem",
    "rootCertificate2File": "../keys/ROT3_p384.pem",
    "rootCertificate3File": "../keys/ROT4_p384.pem",
    "rootCertificateEllipticCurve": "secp384r1",
    "mainRootCertId": 0,
    "mainRootCertPrivateKeyFile": "../keys/ROT1_p384.pem",
    "useIsk": false,
    "signingCertificateConstraint": "0x0000",
    "commands": [
        {
            "fillMemory": {
                "address": "0x2000BF00",
                "pattern": "0x50000000",
                "size": "4"
            }
        },
        {
            "fillMemory": {
                "address": "0x2000BF04",
                "pattern": "0x00000000",
                "size": "4"
            }
        },
        {
            "fillMemory": {
                "address": "0x2000BF08",
                "pattern": "0x00038000",
                "size": "4"
            }
        },
        {
            "configureMemory": {
                "configAddress": "0x2000BF00",
                "memoryId": "0"
            }
        },
        {
            "checkFwVersion": {
                "counterId": "secure",
                "value": "0x1"
            }
        },
        {
            "checkFwVersion": {
                "counterId": "nonsecure",
                "value": "0x1"
            }
        },
        {
            "erase": {
                "address": "0x10000000",
                "size": "0x00038000"
            }
        },
        {
            "load": {
                "address": "0x10000000",
                "file": "../bootable_images/tfm_secureboot.bin"
            }
        }
    ],
    "containerOutputFile": "../bootable_images/tfm_secureboot.sb"
}